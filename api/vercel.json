{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users/(.*)",  
        "dest": "src/routes/users.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }
  